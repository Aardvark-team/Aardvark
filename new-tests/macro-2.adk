macro CHECK(x) {
    (return x) if type_of(x) == Array else x
}
function other() return 6
function test() {
    let my_value = CHECK(other())
    stdout.write(my_value, "\n")
    stdout.write(type_of("Hello") == String, "\n")
    let value_two = CHECK("Hello World")

    return 0
}
stdout.write(test(), "\n")