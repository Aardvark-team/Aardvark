let ESC = ""

class Style as this {
    $constructor(modifiers) {
        this.modifiers = modifiers
    }

    $call(String text) {
        let prefix = ""

        for modifier in this.modifiers {
            prefix += modifier
        }

        return prefix + text + $"{ESC}[0m"
    }

    $getitem(name) {
        stdout.write($"get item '{name}'")
    }
}

let style = Style([])

if is_main {
    stdout.write(style["item"])

    stdout.write($"{style.bold('Hello, World!')}")
}